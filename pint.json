{
    "preset": "laravel",
    "rules": {
        "binary_operator_spaces": {
            "operators": {
                "=>": "align_single_space_minimal"
            }
        },
        "concat_space": {
            "spacing": "one"
        },
        "not_operator_with_successor_space": false,
        "no_superfluous_phpdoc_tags": false,
        "phpdoc_align": {
            "align": "left"
        },
        "phpdoc_separation": {
            "groups": [["param"], ["return"], ["throws"]]
        }
    },
    "exclude": []
}
